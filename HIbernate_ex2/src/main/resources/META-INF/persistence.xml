  <persistence-unit name="com.mycompany_HIbernate_jar_1.0-SNAPSHOTPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.mycompany.hibernate.Helloworld</class>
